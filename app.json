{
  "expo": {
    "name": "Inspeção Elétrica",
    "slug": "inspecao-eletrica",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "inspecao-eletrica",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.empresa.inspecaoeletrica"
    },
    "android": {
      "package": "com.empresa.inspecaoeletrica",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-camera",
      "expo-location",
      "expo-file-system",
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir acesso às fotos para anexar evidências das inspeções.",
          "savePhotosPermission": "Permitir salvar fotos das inspeções na galeria."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}